{
  "name": "flour-bluff-community-center",
  "version": "1.0.0",
  "description": "A place for anyone to come learn.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "css:postcss": "postcss ./css/style.css --use autoprefixer --no-map -o ./css/style.css -w",
    "dev": "concurrently \"npm run watch:sass\" \"npm run css:postcss\" ",
    "compress:sass": "sass --no-source-map   css/style.css css/style.css --style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project423/community-center.git"
  },
  "keywords": [
    "Learn",
    "Fun",
    "Community",
    "Adult",
    "Learning",
    "GED",
    "English",
    "Math",
    "Science",
    "Social",
    "Studies",
    "Reading"
  ],
  "author": "Andrew Clark",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/project423/community-center/issues"
  },
  "type":"module",
  "homepage": "https://github.com/project423/community-center#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "imagemin": "^8.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0"
  },
  "dependencies": {
    "sass": "^1.65.1"
  }
}
